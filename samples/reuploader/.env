TRANSMISSION_PORT_51413=51413

# Deluge Ports
DELUGE_PORT_6881=6881
DELUGE_PORT_8112=8112
DELUGE_PORT_58846=58846

# GG-BOT Visor Server Ports
VISOR_SERVER_PORT=30035

# Services image versions
MONGO_VERSION=latest
DELUGE_VERSION=latest
RUTORRENT_VERSION=latest
QBITTORRENT_VERSION=latest
TRANSMISSION_VERSION=latest
MONGO_EXPRESS_VERSION=latest
GG_BOT_REUPLOADER_VERSION=latest-reuploader
