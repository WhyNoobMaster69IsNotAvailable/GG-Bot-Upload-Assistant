{
    "name": "AsianCinema",
    "url": "https://asiancinema.me",
    "upload_form": "https://asiancinema.me/api/torrents/upload?api_token={api_key}",
    "torrents_search": "https://asiancinema.me/api/torrents/filter?api_token={api_key}",
    "source": "acm",
    "platform": "UNIT3D",
    "bbcode_line_break": "<br />",
    "title_separator" : " ",

    "description_components":{
        "code_code": "[code][/code]",
        "plain_text_code":"",
        "spoiler_code":"[spoiler=TITLE_PLACEHOLDER][/spoiler]",
        "notes_code":"[note][/note]"
    },

    "technical_jargons": {
        "authentication_mode": "API_KEY",
        "payload_type": "MULTI-PART"
    },

    "hybrid_type": {
        "required": false
    },

    "translation": {
        "dot_torrent": "torrent",
        "torrent_title": "name",
        "description": "description",
        "mediainfo": "mediainfo",
        "bdinfo": "bdinfo",
        "type": "category_id",
        "source": "type_id",
        "resolution": "resolution_id",
        "tmdb": "tmdb",
        "imdb": "imdb",
        "anon": "anonymous",
        "live": "live",

        "sd": "sd",
        "tvdb": "tvdb",
        "mal": "mal",
        "igdb": "igdb",
        "optimized": "stream",
        "nfo_file": "nfo_file"
    },

    "Required": {
        "torrent": "file",
        "name": "string",
        "description": "string",
        "sd": "string",
        "anonymous": "string",
        "stream": "string",
        "igdb": "string",
        "mal": "string",
        "tvdb": "string",

        "category_id": {
            "1": "movie",
            "2": "episode"
        },

        "type_id": {

            "1": {
                "bluray_disc": 1,
                "2160p": 1,
                "uhd_100": 1
            },

            "2": {
                "bluray_disc": 1,
                "2160p": 1,
                "uhd_66": 1
            },

            "3": {
                "bluray_disc": 1,
                "2160p": 1,
                "uhd_50": 1
            },

            "4": {
                "bluray_disc": 1,
                "bd_50": 1,
                "1080p": 2,
                "1080i": 2

            },

            "5": {
                "bluray_disc": 1,
                "bd_25": 1,
                "1080p": 2,
                "1080i": 2
            },

            "12": {
                "bluray_remux": 1,
                "2160p": 1
            },

            "7": {
                "bluray_remux": 1,
                "1080p": 2,
                "1080i": 2
            },

            "8": {
                "bluray_disc": 1,
                "2160p": 1
            },

            "9": {
                "webdl": 1
            },

            "10": {
                "bluray_disc": 1,
                "1080p": 2,
                "1080i": 2
            },

            "11": {
                "720p": 1
            },

            "13": {
                "bluray_disc": 1,
                "576p": 2,
                "576i": 2,
                "480p": 2,
                "480i": 2
            },

            "17": {
                "hdtv": 1
            }
        },

        "resolution_id": {
            "1": {
                "2160p": 1
            },
            "2": {
                "1080": 1,
                "1080p": 2,
                "1080i": 2
            },
            "3": {
                "720p": 1
            },
            "4": {
                "576": 1,
                "576p": 2,
                "576i": 2
            },
            "5": {
                "480": 1,
                "480p": 2,
                "480i": 2
            }
        },

        "tmdb": "string",
        "imdb": "string"
    },

    "Optional": {
        "nfo_file": "file",
        "season_number": "string",
        "episode_number": "string",

        "bdinfo": "string",
        "mediainfo": "string"
    },

    "dupes": {
        "request": "GET",
        "strip_text": true,
        "url_format": "{search_url}&imdb={imdb}",

        "parse_json": {
            "is_needed": true,
            "top_lvl": "data",
            "torrent_details": "attributes"
        },

        "different_cuts": "1",
        "2160p_remux": 1,
        "2160p_encode": 1,
        "1080p_remux": 1,
        "1080p_encode": 100,
        "720p_encode": 100,
        "else": 1
    },

    "torrent_title_format": {
        "episode": {
            "bluray_disc": "{title} {year} {s00e00} {screen_size} {region} {source} {dv} {hdr} {video_codec} {audio_codec} {audio_channels} {atmos} {release_group}",
            "bluray_remux": "{title} {year} {s00e00} {repack} {screen_size} {uhd} {hybrid} {source} {remux} {dv} {hdr} {video_codec} {audio_codec} {audio_channels} {atmos} {release_group}",
