{
    "name": "GreatPosterWall",
    "url": "https://greatposterwall.com",
    "upload_form": "https://greatposterwall.com/api.php?api_key={api_key}&action=upload",
    "torrents_search": "https://greatposterwall.com/api.php?api_key={api_key}&action=torrent&req=torrent",

    "source": "GPW",
    "platform": "Gazelle",
    "bbcode_line_break": "\n",
    "title_separator" : " ",

    "description_components":{
        "code_code": "[quote][/quote]",
        "quote_code":"[quote][/quote]"
    },

    "technical_jargons": {
        "authentication_mode": "API_KEY",
        "payload_type": "MULTI-PART",
        "response_type": "TEXT",
        "response_action": "modules.custom_actions.gpw_actions.check_successful_upload",
        "custom_actions": [
            "modules.custom_actions.gpw_actions.check_for_existing_group",
            "modules.custom_actions.gpw_actions.rehost_screens",
            "modules.custom_actions.gpw_actions.rewrite_description",
            "modules.custom_actions.gpw_actions.add_subtitle_information",
            "modules.custom_actions.ptp_actions.mark_scene_release_if_applicable"
        ]
    },

    "hybrid_mappings": {
        "processing":{
            "required": false,
            "prerequisite": [
                "source"
            ],
            "mapping":{
                "Encode":{
                    "source":{
                        "data_source": "tracker",
                        "values": ["Blu-ray"]
                    },
                    "type_id":{
                        "data_source": "tracker",
                        "values": ["3"]
                    }

                },
                "Remux":{
                    "source":{
                        "data_source": "tracker",
                        "values": ["Blu-ray"]
                    },
                    "type_id":{
                        "data_source": "tracker",
                        "values": ["2"]
                    }
                }
            }
        },
        "source": {
            "required": true,
            "prerequisite": [
                "type_id"
            ],
            "mapping":{
                "Blu-ray":{
                    "source":{
                        "data_source": "torrent",
                        "values": ["BluRay", "Blu-Ray"]
                    },
                    "type_id":{
                        "data_source": "tracker",
                        "values": ["2", "3"]
                    }
                },
                "WEB":{
                    "type_id":{
                        "data_source": "tracker",
                        "values": ["4", "5"]
                    }
                },
                "HDTV":{
                    "type_id":{
                        "data_source": "tracker",
                        "values": ["6"]
                    }
                }
            }
        },
        "container":{
            "required":true,
            "prerequisite":[],
            "mapping":{
                "MKV":{
                    "container":{
                        "data_source": "torrent",
                        "values":[".mkv"]
                    }
                },
                "MP4":{
                    "container":{
                        "data_source": "torrent",
                        "values":[".mp4"]
                    }
                }
            }
        },
        "codec":{
            "required":true,
            "prerequisite":[],
            "mapping":{
                "H.264":{
                    "pymediainfo_video_codec":{
                        "data_source": "torrent",
                        "values":["AVC", "H.264"]
                    }
                },
                "x264":{
                    "pymediainfo_video_codec":{
                        "data_source": "torrent",
                        "values":["x264"]
                    }
                },
                "H.265":{
                    "pymediainfo_video_codec":{
                        "data_source": "torrent",
                        "values":["HEVC", "H.265"]
                    }
                },
                "x265":{
                    "pymediainfo_video_codec":{
                        "data_source": "torrent",
                        "values":["x265"]
                    }
                }
            }
        }
    },

    "translation": {
        "dot_torrent": "file_input",
        "hybrid_type_1": "source",
        "hybrid_type_2": "processing",
        "hybrid_type_3": "codec",
        "hybrid_type_4": "container",
        "description": "release_desc",
        "tags": "remaster_title",
        "imdb": "imdb",
        "scene": "scene",

        "torrent_title": "name",
        "mediainfo": "mediainfo[]",
        "bdinfo": "mediainfo[]",
        "type": "category_id",
        "source": "type_id",
        "resolution": "resolution",
        "anon": "anonymous",
        "live": "live",
        "sd": "sd",
        "nfo_file": "nfo_text"
    },

    "Required": {
        "file_input": "file",
        "release_desc": "string",
        "source": "hybrid_type",
        "codec": "hybrid_type",
        "container": "hybrid_type",

        "name": "string",
        "anonymous": "string",
        "sd": "string",
        "stream": "string",
        "imdb": "url",

        "internal": "string",
        "free": "string",
        "featured": "string",
        "doubleup": "string",
        "sticky": "string",

        "category_id": {
            "1": "movie"
        },

        "type_id": {
            "1": {
                "disc": 1,
                "bluray_disc": 2,
                "dvd_disc": 2
            },
            "2": {
                "remux": 1,
                "bluray_remux": 2,
                "dvd_remux": 2
            },
            "3": {
                "bluray_encode": 1
            },
            "4": {
                "webdl": 2,
                "web_dl": 2
            },
            "5": {
                "webrip": 2,
                "web_rip": 2
            },
            "6": {
                "hdtv": 1
            }
        },

        "resolution": {
            "2160p": {
                "2160p": 1
            },
            "1080p": {
                "1080p": 1
            },
            "1080i": {
                "1080i": 1
            },
            "720p": {
                "720p": 1
            },
            "576p": {
                "576p": 2,
                "576i": 2
            },
            "480p": {
                "480p": 2,
                "480i": 2
            },
            "Other": {
                "other": 2,
                "4360p": 2
            }
        }
    },

    "Optional": {
        "nfo_text": "string",
        "scene": "string",
        "processing": "hybrid_type",

        "season_number": "string",
        "episode_number": "string",

        "bdinfo": "string",
        "mediainfo[]": "string",

        "remaster_title": {
            "type" : "string",
            "separator": " / ",
            "tags": [
                "director_s_cut",
                "masters_of_cinema",
                "the_criterion_collection",
                "warner_archive_collection",
                "extended_edition",
                "theatrical_cut",
                "uncut",
                "rifftrax",
                "10_bit",
                "unrated",
                "remaster",
                "hdr10",
                "hdr10plus",
                "dts_x",
                "dolby_vision",
                "dolby_atmos",
                "4k_restoration",
                "4k_remaster",
                "with_commentary",
                "dual_audio",
                "2d_3d_edition",
                "3d_anaglyph",
                "3d_full_sbs",
                "3d_half_ou",
                "3d_half_sbs",
                "2_disc_set",
                "2_in_1",
                "english_dub",
                "extras"
            ]
        }
    },

    "Default":{
        "submit" : "true",
        "remaster_year": "",
        "movie_edition_information": "",
        "scene": "",
        "releasetype" : "",
        "subname" : "",
        "image" : "",
        "year" : "",
        "tags" : "",
        "maindesc" : "",
        "desc" : "",
        "artist_ids[]" : "",
        "artists[]" : "",
        "artists_sub[]" : "",
        "importance[]" : "",
        "name" : "",
        "groupid" : "",
        "subtitle_type": "",
        "subtitles[]": ""
    },

    "dupes": {
        "technical_jargons": {
            "authentication_mode": "API_KEY",
            "payload_type": "MULTI-PART",
            "request_method": "GET",
            "response_type": "JSON"
        },

        "url_format": "{search_url}&imdbID={imdb}",
        "strip_text": false,

        "parse_json": {
            "is_needed": true,
            "top_lvl": "response",
            "torrent_name": "Name",
            "combine_fields": true,
            "fields": [ "Name", "Year", "Resolution", "Source", "Processing", "RemasterTitle", "Codec", "Container"]
        }
    },

    "torrent_title_format": {
        "movie": {
            "bluray_disc": "{title} {year} {edition} {repack} {screen_size} {region} {source} {dv} {hdr} {video_codec} {audio_codec} {audio_channels} {atmos} {release_group}",
            "bluray_remux": "{title} {year} {edition} {repack} {screen_size} {uhd} {hybrid} {source} {remux} {dv} {hdr} {video_codec} {audio_codec} {audio_channels} {atmos} {release_group}",
            "bluray_encode": "{title} {year} {edition} {repack} {screen_size} {uhd} {hybrid} {source} {audio_codec} {audio_channels} {atmos} {dv} {hdr} {video_codec} {release_group}",
            "web": "{title} {year} {edition} {repack} {screen_size} {hybrid} {web_source} {web_type} {audio_codec} {audio_channels} {atmos} {dv} {hdr} {video_codec} {release_group}",
            "hdtv": "{title} {year} {edition} {repack} {screen_size} {hybrid} {source} {audio_codec} {audio_channels} {atmos} {video_codec} {release_group}",
            "dvd": "{title} {year} {edition} {repack} {screen_size} {region} {hybrid} {source} {video_codec} {remux} {audio_codec} {audio_channels} {atmos} {release_group}"
        }
    },

