    "Default":{},

    "dupes": {

        "technical_jargons": {
            "authentication_mode": "API_KEY",
            "payload_type": "MULTI-PART",
            "request_method": "GET",
            "response_type": "JSON"
        },

        "url_format": "{search_url}&imdbId={imdb}",
        "strip_text": true,

        "parse_json": {
            "is_needed": true,
            "top_lvl": "data",
            "torrent_details": "attributes"
        }
    },

    "torrent_title_format": {
        "episode": {
            "bluray_disc": "{title} {year} {s00e00} {!language_str_if_foreign} {screen_size} {region} {source} {dv} {hdr} {video_codec} {audio_codec} {audio_channels} {atmos} {release_group}",
            "bluray_remux": "{title} {year} {s00e00} {!language_str_if_foreign} {repack} {screen_size} {uhd} {hybrid} {source} {remux} {dv} {hdr} {video_codec} {multiaudio} {dualaudio} {audio_codec} {audio_channels} {atmos} {release_group}",
            "bluray_encode": "{title} {year} {s00e00} {!language_str_if_foreign} {repack} {screen_size} {uhd} {hybrid} {source} {multiaudio} {dualaudio} {audio_codec} {audio_channels} {atmos} {dv} {hdr} {video_codec} {release_group}",
