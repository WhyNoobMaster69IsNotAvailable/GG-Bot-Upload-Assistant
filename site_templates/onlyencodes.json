{
    "name": "OnlyEncodes",
    "url": "https://onlyencodes.cc",
    "upload_form": "https://onlyencodes.cc/api/torrents/upload?api_token={api_key}",
    "torrents_search": "https://onlyencodes.cc/api/torrents/filter?api_token={api_key}",
    "source": "OE",
    "platform": "UNIT3D",
    "bbcode_line_break": "\n",
    "title_separator" : " ",
    "screenshot_type": "bbcode_images",

    "description_components":{
        "code_code": "[code][/code]",
        "spoiler_code":"[spoiler=TITLE_PLACEHOLDER][/spoiler]",
        "notes_code":"[note][/note]",
        "quote_code":"[quote][/quote]",
        "alert_code":"[alert][/alert]"
    },

    "technical_jargons": {
        "authentication_mode": "API_KEY",
        "response_type": "JSON",
        "payload_type": "MULTI-PART"
    },

    "hybrid_mappings": {
        "type_id": {
            "required": true,
            "prerequisite": [
                "source",
                "category_id"
            ],
            "mapping": {
                "10":{
                    "_comment": "x265",
					    "category_id": {
                        "data_source": "tracker",
                        "values": [
                            "2", "1"
                        ]
                    },
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "3", "5"
                        ]
                    },
                    "video_codec":{
                        "data_source": "torrent",
                        "values": [
                            "x265", "x.265", "x 265", "H.265"
                        ]
                    }
                },
                "15":{
                    "_comment": "x264",
                  	"category_id": {
                        "data_source": "tracker",
                        "values": [
                            "2", "1"
                        ]
                 },
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "3", "5"
                        ]
                    },
                    "video_codec":{
                        "data_source": "torrent",
                        "values": [
                            "x264", "x.264", "x 264"
                        ]
                    }
                },
                "14":{
                    "_comment": "av1",
                    	"category_id": {
                        "data_source": "tracker",
                        "values": [
                            "2", "1"
                        ]
                 },
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "3", "5"
                        ]
                    },
                    "video_codec":{
                        "data_source": "torrent",
                        "values": [
                            "AV1"
                        ]
                    }
                },
                "19": {
                    "_comment": "FullDisk",
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "1"
                        ]
                    }
                },
                "20": {
                    "_comment": "Remux",
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "2"
                        ]
                    }
                },
                "21": {
                    "_comment": "WEB-DL",
                    "source": {
                        "data_source": "tracker",
                        "values": [
                            "4"
                        ]
                    }
                }

            }
        }
    },

