        "nfo_file": "file",
        "season_number": "string",
        "episode_number": "string",

        "bdinfo": "string",
        "mediainfo": "string"
    },

    "dupes": {
        "request": "GET",
        "url_format": "{search_url}&filter={filter}",
        "strip_text": false,

        "parse_json": {
            "is_needed": true,
            "top_lvl": "data"
        },

        "different_cuts": "1",
        "2160p_remux": 1,
        "2160p_encode": 1,
        "1080p_remux": 1,
        "1080p_encode": 100,
        "720p_encode": 100,
        "else": 1
    },

